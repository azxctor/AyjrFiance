<div class="row pd">
  <div class="col-md-6 col-md-offset-3">
  	  <div class="main-wrapper">
	  		<div class="main">
	  			 <form id="password-form" role="form" method="POST" action="$link.contextPath/web/esw/account/signup">
						<label class="control-label col-md-3" for="newPassword">支付密码</label>
					 	  <div class="input-group col-md-9 form-item has-feedback">
							  <span class="input-group-addon"><i class="fa fa-lock"></i></span>
							  <input type="password" class="form-control" id="newPassword" name="payPwd" minlength="1" maxlength="20" required placeholder="支付密码"/>
						  </div>
						  
						  <label class="control-label col-md-3" for="confirmPassword">确认支付密码</label>
						  <div class="input-group col-md-9 form-item has-feedback">
							  <span class="input-group-addon"><i class="fa fa-lock"></i></span>
							  <input type="password" class="form-control" id="confirmPassword" name="confirmPayPwd" equalTo="#newPassword" minlength="1" maxlength="20" required placeholder="确认支付密码"/>
						  </div>
						  <div class="form-btn-wrapper has-feedback">
								<button type="submit" class="btn btn-mf-primary" id="reg-btn">激活账户</button>
                          </div>
					</form>
	  		</div>
	  		<br/>
	  		<br/>
	  		<br/>
  	   </div>
  </div>
</div>


