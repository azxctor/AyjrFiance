<div class="modal modal-micro fade" id="modal-log" tabindex="-1" role="dialog" aria-labelledby="logModalLabel" aria-hidden="true">
    <div class="modal-dialog" style="width: 65%;">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title" id="logModalLabel">日志</h4>
            </div>
            <div class="modal-body">
                <div class="modal-table">
                    <table class="display" id="table-log" cellspacing="0" cellpadding="0" border="0">
                        <thead>
                            <tr>
                                <th data-mData="applDt" data-bSortable="false">申请时间</th>
                                <th data-mData="createOpid" data-bSortable="false">申请人</th>
                                <th data-mData="trxMemo" data-bSortable="false" style="width: 20%;">申请理由</th>
                                <th data-mData="applStatus.text" data-bSortable="false">复核状态</th>
                                <th data-mData="apprOpid" data-bSortable="false">复核人</th>
								<th data-mData="trxDt" data-bSortable="false">复核时间</th>
								<th data-mData="dealMemo" data-bSortable="false" style="width: 20%;">复核结果</th>
                            </tr>
                        </thead>
                        <tbody>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
